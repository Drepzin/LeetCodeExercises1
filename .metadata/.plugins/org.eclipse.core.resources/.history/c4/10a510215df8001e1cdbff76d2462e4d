package funcoesDeTest;

public class leetCode1 {

	public int removeElement(int[] nums, int val) {

		int k = 0, len = nums.length;
		for (int i = 0; i < len; i++) {
			if (nums[i] == val) {
				k++;
				for (int l = len - 1; l >= 0; l--) {
					if (nums[l] != val) {
						nums[i] = nums[l];
						nums[l] = val;
					}
				}
			}
		}
		return k;
	}
}
